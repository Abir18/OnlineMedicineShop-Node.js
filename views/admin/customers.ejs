<!DOCTYPE html>
<html>
<head>
	<title>User List</title>
</head>
<body>
	<h1>User List</h1>

	<a href="/admin">Back</a> |
	<a href="/">logout</a> 
	<br>
	<br>

	<table border="1" width="400px" >
		<tr>
			<th>ID</th>
			<th>USER NAME</th>
			<th>USER PASSWORD</th>
            <th>ACTION</th>
			

		</tr>

		<% for(var i=0; i < uList.length; i++){ %>
		<tr>
			<td><%= uList[i].id %></td>
			<td><%= uList[i].username %></td>
			<td><%= uList[i].password %></td>
            <td>
                <a href="/deleteuser/<%= uList[i].id %>">DELETE</a>
            </td>
		</tr>
		<% } %>
	</table>
</body>
</html>